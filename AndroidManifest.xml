            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".editor.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
    </application>

</manifest>